        "target": "es5",
        "module": "commonjs",
        "outDir": "public/js",
        "rootDir": "src",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": false
    },
    "include": ["src"]
